{
    "manifest_version": 3,
    "name": "ATB - Auto Benedu",
    "description": "한국디지털미디어고등학교 베네듀 자동 풀이",
    "version": "2.0",
    "content_scripts": [
        {
            "matches": ["https://benedu.co.kr/StudentStudy/*"],
            "js": ["answer.js"]
        },
        {
            "matches": ["https://benedu.co.kr/*"],
            "js": ["/popup/js/index.js"]
        },
        {
            "matches": ["https://benedu.co.kr/StudentStudy/SearchQuestion"],
            "js": ["makeTest.js"]
        }
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_icons": {
            "16": "icon.png",
            "24": "icon.png",
            "32": "icon.png"
        },
        "default_popup": "/popup/index.html",
        "default_title": "ATB - Auto Benedu"
    },
    "permissions": [
        "storage"
    ]
}
